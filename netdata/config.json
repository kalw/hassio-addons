{
  "name": "Netdata",
  "version": "dev",
  "slug": "netdata",
  "description": "Monitor everything in real time â€“ for free",
  "url": "https://github.com/kalw/hassio-addons/",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:19999]",
  "ingress": true,
  "ingress_port": 19999,
  "panel_icon": "mdi:lens",
  "startup": "system",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "map": ["ssl"],
  "ports": {
    "19999/tcp": null
  },
  "ports_description": {
    "19999/tcp": "Web interface (Not required for Ingress)"
  },
  "options": {
    "culture": "en",
    "telemetry": "true", 
    "updates": "false", 
    "ssl": "true",
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "culture": "list(cs|da|de|en|en_GB|es|fr|hu|it|ja|nl|no|pl|pt_BR|pt_PT|ru|sk_SK|sv_SE|tr|zh_TW)",
    "telemetry": "bool",
    "updates": "bool",
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str"
  }
}
